Cell[CellGroupData[{
Cell["\<\
ME 201/MTH 281
Assignment #11 Solutions\
\>", "Title"],

Cell[CellGroupData[{

Cell["Problem 1 d", "SectionFirst"],

Cell["\<\
\tWe get the first 20 zeros of J0 and put them in the array \[Alpha]zer.\
\>", "Text",
 CellChangeTimes->{3.393237185319071*^9}],

Cell["\<\
\[Alpha]zer = Table[N[BesselJZero[0,i]],{i,1,20}];\
\>", "Input",
 CellChangeTimes->{{3.3932371899026127`*^9, 3.3932372036784973`*^9}, {
  3.393237234894734*^9, 3.393237273343251*^9}},
 AspectRatioFixed->True,
 CellLabel->"In[2]:="],

Cell["\<\
We define numerically the integrals appearing in the expression for c[[n]].  \
We start with the numerator.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "r_", "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{"1", "-", 
    RowBox[{
     RowBox[{"r", "^", "2"}], "/", 
     RowBox[{"a", "^", "2"}]}]}], ")"}]}]], "Input",
 CellLabel->"In[3]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"num", "[", "n_", "]"}], ":=", 
  RowBox[{"\[CapitalPhi]0", " ", 
   RowBox[{"NIntegrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "r", "]"}], " ", 
      RowBox[{"BesselJ", "[", 
       RowBox[{"0", ",", 
        RowBox[{
         RowBox[{"\[Alpha]zer", "[", 
          RowBox[{"[", "n", "]"}], "]"}], "*", 
         RowBox[{"r", "/", "a"}]}]}], "]"}], " ", "r"}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "a"}], "}"}]}], "]"}]}]}]], "Input",
 CellLabel->"In[4]:="],

Cell["Now the denominator.", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"den", "[", "n_", "]"}], ":=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"BesselJ", "[", 
        RowBox[{"0", ",", 
         RowBox[{
          RowBox[{"\[Alpha]zer", "[", 
           RowBox[{"[", "n", "]"}], "]"}], "*", 
          RowBox[{"r", "/", "a"}]}]}], "]"}], ")"}], "^", "2"}], " ", "r"}], 
    ",", " ", 
    RowBox[{"{", 
     RowBox[{"r", ",", "0", ",", "a"}], "}"}]}], "]"}]}]], "Input",
 CellLabel->"In[5]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"fbc", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"num", "[", "n", "]"}], "/", 
   RowBox[{"den", "[", "n", "]"}]}]}]], "Input",
 CellLabel->"In[6]:="],

Cell["We set the parameter values.", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[CapitalPhi]0", "=", "100"}], " ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "volts"}], " ", "**)"}], ";", 
  RowBox[{"a", "=", "2"}], " ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "m"}], " ", "**)"}], ";"}]], "Input",
 CellLabel->"In[7]:="],

Cell["\<\
Now we calculate the first 20 coefficients and store them in the array coeff.\
\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"coeff", "=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "ans", "}"}], ",", 
    RowBox[{
     RowBox[{"ans", "=", 
      RowBox[{"{", "}"}]}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"ans", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"ans", ",", 
          RowBox[{"fbc", "[", "n", "]"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", "20"}], "}"}]}], "]"}], ";", "ans"}]}], 
   "]"}]}]], "Input",
