 CellChangeTimes->{{3.4585660100731773`*^9, 3.458566014911977*^9}}],

Cell[TextData[{
 "\tNow we write the module that gives us the decay time tau in hours from a \
given value of ",
 StyleBox["h",
  FontSlant->"Italic"],
 " in W/m\[CenterDot]K.  First we clear the previously assigned value of ",
 StyleBox["h",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.4585660354803877`*^9, 3.458566102802041*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Clear", "[", "h", "]"}], ";"}]], "Input",
 CellChangeTimes->{{3.458566105419339*^9, 3.4585661106346273`*^9}},
 CellLabel->"In[68]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"tau", "[", "h_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"Bi", ",", "z", ",", "\[Lambda]"}], "}"}], ",", 
    RowBox[{
     RowBox[{"Bi", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"h", " ", "L"}], ")"}], "/", "k"}]}], ";", 
     RowBox[{"z", "=", 
      RowBox[{"z1", "[", "Bi", "]"}]}], ";", 
     RowBox[{"\[Lambda]", "=", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"z", "/", "L"}], ")"}], "2"]}], ";", " ", 
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{"3600", "*", "\[Lambda]", "*", "Df"}], ")"}]}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.4585660818134193`*^9, 3.4585660828871193`*^9}, {
  3.458566116011867*^9, 3.458566291262928*^9}},
