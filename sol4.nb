Cell["\tThe Fourier sine coefficient is", "Text",
 CellChangeTimes->{{3.455297648033536*^9, 3.455297654345511*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"coeff", "[", "n_", "]"}], "=", 
  RowBox[{"Simplify", "[", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"2", " ", "To"}], "a"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "a"], 
      RowBox[{
       FractionBox["x", "a"], 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         FractionBox["x", "a"]}], ")"}], 
       RowBox[{"Sin", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", " ", "\[Pi]", " ", "x"}], ")"}], "/", "a"}], "]"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}]}], ",", 
    RowBox[{"n", "\[Element]", " ", "Integers"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.455297669976749*^9, 3.455297791893608*^9}, {
  3.455297892681123*^9, 3.455297893209161*^9}, {3.4552993124968147`*^9, 
  3.4552993407213783`*^9}},
 CellLabel->"In[137]:="]
}, Open  ]]
