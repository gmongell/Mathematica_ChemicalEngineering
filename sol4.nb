Cell["\<\
\tBefore constructing the graphs, we must specify the parameter values.\
\>", "Text",
 CellChangeTimes->{{3.455293938633438*^9, 3.455293953296567*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Beta]", "=", " ", "4.8"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"\[Degree]C", "/", "s"}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"L", "=", "0.1"}], " ", 
  RowBox[{"(*", " ", "m", " ", "*)"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Df", " ", "=", " ", 
   RowBox[{"2.0", "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "4"}]]}]}], "  ", 
  RowBox[{"(*", " ", 
   RowBox[{
    SuperscriptBox["m", "2"], "/", "s"}], " ", "*)"}], 
  ";"}], "\[IndentingNewLine]", 
