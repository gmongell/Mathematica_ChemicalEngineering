Cell[CellGroupData[{

Cell["Problem 3 (g)", "SectionFirst",
 CellChangeTimes->{{3.4550371734449787`*^9, 3.4550371755656776`*^9}, {
  3.45529265431549*^9, 3.4552926616503267`*^9}}],

Cell[TextData[{
 "\tWe construct here the graphs asked for in problem (3) (g).  You were \
asked only for the graph of T.  We will give a more complete picture here by \
including ",
 StyleBox["T ",
  FontSlant->"Italic"],
 "in blue, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"T0", " ", 
    RowBox[{"(", 
     RowBox[{"the", " ", "initial", " ", "temperature"}], ")"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 " in green, and ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"Ts", " ", 
     RowBox[{"(", 
      RowBox[{"steady", " ", "state"}], ")"}], " "}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "in red.  We start by defining these functions.  We start with ",
 Cell[BoxData[
  FormBox["Ts", TraditionalForm]]],
 ".  We use Df rather than D for diffusivity, because D is a reserved symbol \
in Mathematica."
}], "Text",
 CellChangeTimes->{{3.455292669586021*^9, 3.455292817311974*^9}, {
  3.455292863014824*^9, 3.455292938047798*^9}, {3.455293040272653*^9, 
  3.455293049744835*^9}, {3.455293684157715*^9, 3.4552937221164494`*^9}, {
  3.455294372889103*^9, 3.455294384891056*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Ts", "[", "x_", "]"}], ":=", 
  RowBox[{
   FractionBox[
    RowBox[{"\[Beta]", " ", 
     SuperscriptBox["L", "2"]}], 
    RowBox[{
     SuperscriptBox["\[Pi]", "2"], "Df"}]], 
   RowBox[{"Sin", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Pi]", " ", "x"}], ")"}], "/", "L"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.455292941890135*^9, 3.455292945863741*^9}, {
  3.455293010711619*^9, 3.455293083506366*^9}, {3.455294924811018*^9, 
  3.4552949355564528`*^9}},
 CellLabel->"In[119]:="],

Cell["\<\
Now we define the transient.  We start with the coefficients in the series.  \
We index the coefficients by 2k + 1, with k running through all integers \
starting at zero.\
\>", "Text",
 CellChangeTimes->{{3.4552931324999638`*^9, 3.455293149570505*^9}, {
  3.455293279373075*^9, 3.455293304909568*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"c", "[", "0", "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{"4", " ", "T0"}], "\[Pi]"], "-", " ", 
    FractionBox[
     RowBox[{"\[Beta]", " ", 
      SuperscriptBox["L", "2"]}], 
     RowBox[{"Df", " ", 
      SuperscriptBox["\[Pi]", "2"]}]]}]}], ";", 
  RowBox[{
   RowBox[{"c", "[", "k_", "]"}], ":=", 
   FractionBox[
    RowBox[{"4", " ", "T0"}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", " ", "k"}], " ", "+", " ", "1"}], ")"}], 
     "\[Pi]"}]]}]}]], "Input",
 CellChangeTimes->{{3.4552931543382177`*^9, 3.455293256877781*^9}, {
  3.455293309582736*^9, 3.455293388722122*^9}},
 CellLabel->"In[120]:="],

Cell["Now we define the kth term in the series:", "Text",
 CellChangeTimes->{{3.455293402911201*^9, 3.4552934114631443`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"term", "[", 
   RowBox[{"x_", ",", "t_", ",", "k_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"c", "[", "k", "]"}], 
   RowBox[{"Exp", "[", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", "k"}], " ", "+", " ", "1"}], ")"}], "2"], " ", 
       SuperscriptBox["\[Pi]", "2"], " ", "Df", " ", "t"}], 
      SuperscriptBox["L", "2"]]}], "]"}], 
   RowBox[{"Sin", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "k"}], " ", "+", " ", "1"}], ")"}], "\[Pi]", " ", 
       "x"}], ")"}], "/", "L"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.4552934144790697`*^9, 3.4552935972120123`*^9}},
 CellLabel->"In[121]:="],

Cell[TextData[{
 "Now we use the first five nonzero terms to represent ",
 Cell[BoxData[
  FormBox["Ttran", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.455293633003738*^9, 3.455293657692655*^9}, {
  3.455293732623616*^9, 3.4552937333975077`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Ttran", "[", 
   RowBox[{"x_", ",", "t_"}], "]"}], ":=", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{"term", "[", 
     RowBox[{"x", ",", "t", ",", "k"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"k", ",", "0", ",", "4"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.455293736054759*^9, 3.455293761818775*^9}},
 CellLabel->"In[122]:="],

Cell["Now we can define T.", "Text",
 CellChangeTimes->{{3.455293776014485*^9, 3.4552937799494467`*^9}}],

Cell[BoxData[
