ubscript[StringJoin@@Map[Apply[StringJoin,Table[ToString[#[[\1]]],{#[[2]]}]]&,colln]]]];Protect[Derivative];)dformat[Off]:=(Unprotect[Derivative];Format[Derivative[ind___][u_][vars___]]=.;Protect[Derivative];)\\>", "Input",  ImageRegion->{{0, 1}, {0, 1}}],Cell[BoxData[    \("dformat controls the format of partial derivatives.  dformat[On] turns \formatting on,  dformat[Off] turns formatting off."\)], "Output"]}, Open  ]],Cell[CellGroupData[{Cell["examples of formatting", "Subsubsection",  ImageRegion->{{0, 1}, {0, 1}}],Cell[CellGroupData[{Cell[BoxData[    \(Laplacian[u[r, \[Theta], 0]] // Simplify\)], "Input"],Cell[BoxData[    RowBox[{      FractionBox[        RowBox[{          SuperscriptBox["u",             TagBox[\((0, 2, 0)\),              Derivative],            MultilineFunction->None], "[", \(r, \[Theta], 0\),           "]"}], \(r\^2\)], "+",       FractionBox[        RowBox[{          SuperscriptBox["u",             TagBox[\((1, 0, 0)\),              Derivative],            MultilineFunction->None], "[", \(r, \[Theta], 0\), "]"}], "r"],       "+",       RowBox[{        SuperscriptBox["u",           TagBox[\((2, 0, 0)\),            Derivative],          MultilineFunction->None], "[", \(r, \[Theta], 0\),         "]"}]}]], "Output"]}, Open  ]],Cell["dformat[On]", "Input",  ImageRegion->{{0, 1}, {0, 1}}],Cell[CellGroupData[{Cell[BoxData[    \(Laplacian[u[r, \[Theta], 0]] // Simplify\)], "Input"],Cell[BoxData[    RowBox[{      FractionBox[        InterpretationBox[          RowBox[{"u", "\[InvisibleSpace]",             InterpretationBox[\(\[Null]\_"\[Theta]\[Theta]"\),              Subscript[ "\[Theta]\[Theta]"],              Editable->False]}],          SequenceForm[ u,             Subscript[ "\[Theta]\[Theta]"]],          Editable->False], \(r\^2\)], "+",       FractionBox[        InterpretationBox[          RowBox[{"u", "\[InvisibleSpace]",             InterpretationBox[\(\[Null]\_"r