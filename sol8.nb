Cell["\<\
\tWe will use FindRoot here to find the first three frequencies of our \
string-slider system.  We begin by specifying the parameter values.\
\>", "Text",
 CellChangeTimes->{{3.454693472967423*^9, 3.454693589368453*^9}, {
  3.454695834208242*^9, 3.454695885807625*^9}, {3.45477279688174*^9, 
  3.4547728099201603`*^9}, {3.460475780506847*^9, 3.460475817963214*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"L", "=", "0.5"}], " ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "string", " ", "length", " ", "in", " ", "m"}], " ", 
   "**)"}], ";"}]], "Input",
 CellChangeTimes->{{3.460475826991686*^9, 3.460475845508753*^9}},
 CellLabel->"In[1]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Sigma]", "=", 
   RowBox[{"4", "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "3"}]]}]}], "  ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "string", " ", "density", " ", "in", " ", 
    RowBox[{"kg", "/", "m"}]}], " ", "**)"}], ";"}]], "Input",
 CellChangeTimes->{{3.460475868142243*^9, 3.460475918406015*^9}},
 CellLabel->"In[2]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"T", "=", " ", "40.0"}], " ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "string", " ", "tension", " ", "in", " ", "N"}], " ", 
   "**)"}], ";"}]], "Input",
 CellChangeTimes->{{3.46047592401915*^9, 3.460475943622476*^9}, {
  3.4604767721820097`*^9, 3.4604768645353928`*^9}},
 CellLabel->"In[3]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"m", "=", 
   RowBox[{"2", "*", 
    SuperscriptBox["10", 
     RowBox[{"-", "3"}]]}]}], " ", 
  RowBox[{"(*", 
   RowBox[{"*", " ", "slider", " ", "mass", " ", "in", " ", "kg"}], " ", 
   "**)"}], ";"}]], "Input",
 CellChangeTimes->{{3.46047592401915*^9, 3.460475943622476*^9}, {
  3.4604767721820097`*^9, 3.4604768645353928`*^9}},
 CellLabel->"In[4]:="],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Beta]", ":=", 
   RowBox[{"(", 
    RowBox[{"\[Sigma]", " ", 
     RowBox[{"L", "/", "m"}]}], ")"}]}], " ", 
  RowBox[{"(*", 
   RowBox[{
   "*", " ", "ratio", " ", "of", " ", "string", " ", "mass", " ", "to", " ", 
    "slider", " ", "mass"}], " ", "**)"}], ";"}]], "Input",
